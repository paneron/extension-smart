{
  "name": "@paneron/extension-hls",
  "version": "1.0.0-dev19",
  "description": "Paneron extension for SMART model editing",
  "main": "plugin.js",
  "repository": "https://github.com/paneron/extension-hls",
  "author": {
    "name": "Ribose Inc.",
    "email": "open.source@ribose.com"
  },
  "paneronExtension": {
    "title": "SMART",
    "iconURL": "https://www.glossarist.org/SymbolLoupe.png",
    "featured": true,
    "requiredHostAppVersion": "^1.0.0-beta18"
  },
  "dist_dir": "dist",
  "scripts": {
    "build": "yarn run clean; mkdirp compiled ${npm_package_dist_dir} && tsc && babel compiled --out-dir ${npm_package_dist_dir} && cp package.json ${npm_package_dist_dir}/ && rsync -a --include '*/' --include '*.d.ts' --include '*.js.map' --exclude '*' compiled/ ${npm_package_dist_dir}/",
    "buildwin": "yarn run clean & mkdirp compiled %npm_package_dist_dir% & tsc & xcopy /e /y compiled %npm_package_dist_dir% > nul & babel compiled --out-dir %npm_package_dist_dir% & copy package.json %npm_package_dist_dir% & md \"%appdata%\\Paneron\\plugins\\@riboseinc\\paneron-extension-hls\" & xcopy /e /y dist \"%appdata%\\Paneron\\plugins\\@riboseinc\\paneron-extension-hls\" > nul",
    "lint": "eslint src/ --ext .ts,.tsx",
    "clean": "rimraf compiled/* dist/*",
    "test": "jest",
    "fix": "yarn run lint -- --fix"
  },
  "files": [
    "README.adoc",
    "*.js",
    "*.js.map",
    "*.d.ts",
    "**/*.js",
    "**/*.js.map",
    "**/*.d.ts"
  ],
  "resolutions": {
    "@types/react": "16.9.49",
    "react": "16.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@blueprintjs/core": "3.44.1",
    "@blueprintjs/icons": "3.26.0",
    "@blueprintjs/popover2": "0.7.1",
    "@blueprintjs/select": "^3.17.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@react-three/drei": "^7.22.6",
    "@react-three/fiber": "^7.0.19",
    "@riboseinc/paneron-extension-glossarist": "1.0.0-dev34",
    "@riboseinc/paneron-extension-kit": "2.0.0-alpha23",
    "@types/jest": "^27.5.1",
    "@types/node": "^14.11.2",
    "@types/react": "16.9.49",
    "@types/react-reconciler": "^0.26.4",
    "@types/three": "^0.134.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-jest": "^28.1.0",
    "eslint": "^8.16.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "fast-check": "^2.25.0",
    "jest": "^28.1.0",
    "liquidjs": "^9.25.0",
    "mkdirp": "^1.0.4",
    "prop-types": "^15.8.1",
    "react": "16.14.0",
    "react-flow-renderer": "^9.6.0",
    "react-mathjax2": "^0.0.2",
    "rimraf": "^3.0.2",
    "three": "^0.134.0",
    "three-stdlib": "^2.5.9",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4"
  },
  "license": "MIT"
}
