{
  "name": "@paneron/extension-hls",
  "version": "1.0.0-dev19",
  "description": "Paneron extension for SMART model editing",
  "main": "plugin.js",
  "repository": "https://github.com/paneron/extension-hls",
  "author": {
    "name": "Ribose Inc.",
    "email": "open.source@ribose.com"
  },
  "paneronExtension": {
    "title": "SMART",
    "iconURL": "https://www.glossarist.org/SymbolLoupe.png",
    "featured": true,
    "requiredHostAppVersion": "^1.0.0-beta18"
  },
  "dist_dir": "dist",
  "scripts": {
    "build": "yarn run clean; mkdirp compiled ${npm_package_dist_dir} && tsc && babel compiled --out-dir ${npm_package_dist_dir} && cp package.json ${npm_package_dist_dir}/ && rsync -a --include '*/' --include '*.d.ts' --include '*.js.map' --exclude '*' compiled/ ${npm_package_dist_dir}/",
    "buildwin": "yarn run clean & mkdirp compiled %npm_package_dist_dir% & tsc & xcopy /e /y compiled %npm_package_dist_dir% > nul & babel compiled --out-dir %npm_package_dist_dir% & copy package.json %npm_package_dist_dir% & md \"%appdata%\\Paneron\\plugins\\@riboseinc\\paneron-extension-hls\" & xcopy /e /y dist \"%appdata%\\Paneron\\plugins\\@riboseinc\\paneron-extension-hls\" > nul",
    "lint": "eslint src/ --ext .ts,.tsx",
    "clean": "rimraf compiled/* dist/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "fix": "yarn run lint -- --fix"
  },
  "files": [
    "README.adoc",
    "*.js",
    "*.js.map",
    "*.d.ts",
    "**/*.js",
    "**/*.js.map",
    "**/*.d.ts"
  ],
  "resolutions": {
    "@types/react": "16.14.30",
    "react": "16.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@blueprintjs/core": "3.54.0",
    "@blueprintjs/icons": "3.33.0",
    "@blueprintjs/popover2": "0.14.0",
    "@blueprintjs/select": "^3.19.1",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@react-three/drei": "^7.27.5",
    "@react-three/fiber": "^7.0.29",
    "@riboseinc/paneron-extension-glossarist": "1.0.0-dev9",
    "@riboseinc/paneron-extension-kit": "^2.0.0-alpha24",
    "@types/jest": "^28.1.7",
    "@types/node": "^14.18.24",
    "@types/react": "16.14.30",
    "@types/react-reconciler": "^0.28.0",
    "@types/three": "^0.143.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-jest": "^28.1.3",
    "eslint": "^8.22.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-jest": "^26.8.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-check": "^2.25.0",
    "jest": "^28.1.3",
    "liquidjs": "^9.40.0",
    "mkdirp": "^1.0.4",
    "prop-types": "^15.8.1",
    "react": "16.14.0",
    "react-flow-renderer": "^9.7.4",
    "react-mathjax2": "^0.0.2",
    "rimraf": "^3.0.2",
    "three": "^0.143.0",
    "three-stdlib": "^2.13.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "license": "MIT"
}
